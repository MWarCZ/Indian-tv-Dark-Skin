{
  "manifest_version": 2,

  "name": "Indian-tv Dark Skin",
  "description": "xxx",
  "version": "1.1",

  "content_scripts": [
    {
      "matches": ["https://indian-tv.cz/*"],
      "css": ["xxx.css"]
    }
  ],

  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "indian-red.png",
    "default_title": "Indian-tv Dark Skin"
  },

  "permissions": [
    "activeTab",
    "tabs", 
    "https://indian-tv.cz/*",
    "webNavigation"
  ]
}
